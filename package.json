  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "graphql": "*",
    "graphql-relay": "*",
    "material-ui": "*",
    "prop-types": "*",
    "react": "*",
    "react-dom": "*",
    "react-loadable": "*",
    "react-markdown": "^2.5.0",
    "react-relay": "*",
    "react-router-dom": "*",
    "react-tap-event-plugin": "*",
    "recharts": "*"
  },
  "devDependencies": {
    "babel-preset-react-app": "*",
    "flow-bin": "*",
    "get-graphql-schema": "*",
    "js-beautify": "*",
    "relay-compiler": "*",
    "relay-modern-scripts": "*"
  },
  "scripts": {
    "start": "PORT=3333 react-scripts start",
    "build": "react-scripts build",
    "flow": "flow",
